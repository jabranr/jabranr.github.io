{
  "name": "jabranr.github.io",
  "version": "3.0.0",
  "description": "Jabran Rafique's personal website and blog",
  "main": "index.js",
  "scripts": {
    "start": "run-p dev:*",
    "dev:watch": "NODE_ENV=development eleventy --serve",
    "dev:css": "tailwindcss -i ./styles/main.css -o ./public/main.css --watch",
    "build:css": "tailwindcss -m -i ./styles/main.css -o ./public/main.css",
    "build": "npm run build:css && NODE_ENV=production eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jabranr/jabranr.github.io.git"
  },
  "keywords": [
    "blog",
    "website",
    "Jabran",
    "Rafique"
  ],
  "bugs": {
    "url": "https://github.com/jabranr/jabranr.github.io/issues"
  },
  "private": true,
  "homepage": "https://jabran.me",
  "author": {
    "name": "Jabran Rafique",
    "email": "hello@jabran.me"
  },
  "prettier": "@jabranr-lab/prettier",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0",
    "@jabranr-lab/prettier": "^1.2.1",
    "autoprefixer": "^10.4.12",
    "cloudflare": "^2.9.1",
    "dayjs": "^1.11.4",
    "dotenv": "^16.0.1",
    "dotenv-expand": "^9.0.0",
    "eleventy-plugin-time-to-read": "^1.2.0",
    "husky": "^8.0.1",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "postcss-import": "^15.0.0",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "pretty-quick": "^3.1.3",
    "prismjs": "^1.29.0",
    "tailwindcss": "^3.1.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
