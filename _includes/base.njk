{% if not title %}
  {% set title = 'Jabran Rafique - Full Stack Web Engineer' %}
{% else %}
  {% set title = ((renderData and renderData.title) or title) ~ " - Jabran Rafique" %}
{% endif %}

{% if not description %}
  {% set description = 'Full Stack Web Engineer, currently Staff Engineer at Rated People' %}
{% endif %}
<!doctype html>
<html lang="en" xmlns:fb="http://ogp.me/ns/fb#" class="scroll-smooth">
  <head>
    <meta charset="UTF-8" />
    <title>{{ (renderData and renderData.title) or title }}</title>
    <meta name="description" content="{{ (renderData and renderData.description) or description }}" />
    <meta name="keywords" content="blog, jabran, rafique, html, css, javascript, react, php, full stack, Frontend{{ (renderData and ', ' ~ renderData.keywords)  }}" />
    <meta name="generator" content="{{eleventy.generator}}" />
    <meta property="og:type" content="website" />
    <meta property="og:title" content="{{ title }}" />
    <meta property="og:description" content="{{ (renderData and renderData.description) or description }}" />
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:site" content="@jabranr">
    <meta name="twitter:creator" content="@jabranr">
    <meta name="twitter:title" content="{{ title }}">
    <meta name="twitter:description" content="{{ (renderData and renderData.description) or description }}">
    <meta property="og:image" content="{{ '/icon-1024x1024.png' | absoluteUrl }}" />
    <meta name="twitter:image" content="{{ '/icon-1024x1024.png' | absoluteUrl }}" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="msapplication-TileImage" content="{{ '/apple-touch-icon.png' | absoluteUrl }}" />
    <meta name="msapplication-TileColor" content="#222222" />
    <meta name="theme-color" content="#222222" />
    {% if page.url == '/404/' %}
      <meta property="robots" content="noindex, nofollow" />
    {% else %}
      <meta property="robots" content="all" />
      <meta property="og:url" content="{{ page.url | absoluteUrl }}{% if page.url == '/' %}/{% endif %}">
      <link rel="canonical" href="{{ page.url | absoluteUrl }}{% if page.url == '/' %}/{% endif %}" />
    {% endif %}
    <link rel="preconnect" href="https://www.googletagmanager.com" />
    <link rel="preconnect" href="https://www.google-analytics.com" />
    <link rel="manifest" href="{{ '/manifest.json' | absoluteUrl }}?v={{ 'now' | format_date('unix') }}" />
    <link rel="apple-touch-icon" sizes="180x180" href="{{ '/apple-touch-icon.png' | absoluteUrl }}" />
    <link rel="icon" type="image/png" sizes="32x32" href="{{ '/favicon-32x32.png' | absoluteUrl }}" />
    <link rel="icon" type="image/png" sizes="16x16" href="{{ '/favicon-16x16.png' | absoluteUrl }}" />

    <link rel="icon" type="image/x-icon" href="{{ '/favicon.ico' | absoluteUrl }}" />
    <link rel="apple-touch-icon" href="{{ '/apple-touch-icon.png' | absoluteUrl }}" />

    <link href="{{ '/main.css' | absoluteUrl }}?v={{ 'now' | format_date('unix') }}" rel="stylesheet" />

    {% if env_NODE_ENV != 'development' %}
      <!-- Google Tag Manager -->
      <script>(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':
      new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],
      j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.defer=true;j.src=
      'https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);
      })(window,document,'script','dataLayer','{{ env_GTM_ID }}');</script>
      <!-- End Google Tag Manager -->
    {% endif %}
  </head>
  <body class='bg-sky-50 dark:bg-sky-950 text-gray-900 dark:text-sky-50'>
    {% if env_NODE_ENV != 'development' %}
      <!-- Google Tag Manager (noscript) -->
      <noscript><iframe src="https://www.googletagmanager.com/ns.html?id={{ env_GTM_ID }}"
      height="0" width="0" class="invisible hidden"></iframe></noscript>
      <!-- End Google Tag Manager (noscript) -->
    {% endif %}
    {% block schema %}{% endblock schema %}
    <div class="absolute top-4 right-4 ">
      <button type='button' onclick="toggleTheme()" id="theme-toggle" aria-label="Toggle Dark Mode">
        <svg
          data-v-15b35c9e=""
          xmlns="http://www.w3.org/2000/svg"
          width="20"
          height="20"
          viewBox="0 0 24 24"
          fill="currentColor"
          stroke="currentColor"
          stroke-width="2"
          stroke-linecap="round"
          stroke-linejoin="round"
          class="lucide lucide-icon customizable lucide-moon-icon lucide-moon lucide-icon customizable icon-dark-mode"
        >
          <path
            d="M20.985 12.486a9 9 0 1 1-9.473-9.472c.405-.022.617.46.402.803a6 6 0 0 0 8.268 8.268c.344-.215.825-.004.803.401"
          />
        </svg>
        <svg
          data-v-15b35c9e=""
          xmlns="http://www.w3.org/2000/svg"
          width="20"
          height="20"
          viewBox="0 0 24 24"
          fill="currentColor"
          stroke="currentColor"
          stroke-width="2"
          stroke-linecap="round"
          stroke-linejoin="round"
          class="lucide lucide-icon customizable lucide-sun-icon lucide-sun lucide-icon customizable icon-light-mode"
        >
          <circle cx="12" cy="12" r="4" />
          <path d="M12 2v2" />
          <path d="M12 20v2" />
          <path d="m4.93 4.93 1.41 1.41" />
          <path d="m17.66 17.66 1.41 1.41" />
          <path d="M2 12h2" />
          <path d="M20 12h2" />
          <path d="m6.34 17.66-1.41 1.41" />
          <path d="m19.07 4.93-1.41 1.41" />
        </svg>
      </button>
    </div>
    <div class="container mx-auto p-4 max-w-3xl">
      <header class="header my-6">
        {% if page.url != '/' %}
          <a class="logo" href="/">Jabran Rafique</a>
        {% else %}
          <h1 class="logo">Jabran Rafique</h1>
        {% endif %}
        <script>
          function toggleTheme() {
            const html = document.documentElement;
            const currentTheme = html.classList.contains('dark') ? 'dark' : 'light';
            const newTheme = currentTheme === 'dark' ? 'light' : 'dark';

            html.classList.remove(currentTheme);
            html.classList.add(newTheme);
            localStorage.setItem('theme', newTheme);
          }

          // Initialize theme on page load
          (function() {
            const savedTheme = localStorage.getItem('theme') || 'light';
            document.documentElement.classList.add(savedTheme);
          })();
        </script>
      </header>
      <main class="main">{{ content | safe }}</main>
      <footer class="container bg-full-width text-sm py-4">
        <div class="max-w-lg">
          <p>&copy; 2012 &ndash; present</p>
          <div class="colophone">
